{
  "manifest_version": 3,
  "name": "Redmine Time Tracker",
  "description": "Redmine Time Tracker that helps logging hours in Redmine",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Redmine Time Tracker",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}

